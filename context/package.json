{
  "name": "frontend-examples",
  "version": "1.0.0",
  "description": "Frontend examples in various languages",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbdoulSy/frontend-example.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AbdoulSy/frontend-example/issues"
  },
  "homepage": "https://github.com/AbdoulSy/frontend-example#readme",
  "dependencies": {
    "parcel": "^1.12.4",
    "prop-types": "^15.7.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@testing-library/dom": "^6.10.1",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "docsify": "^4.9.4",
    "eslint": "^6.6.0",
    "immer": "^5.0.0",
    "jest": "^24.9.0",
    "jest-css-modules-transform": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.2.0",
    "react-test-renderer": "^16.11.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "storybook": "^5.1.11",
    "stylelint": "^11.1.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "setupFiles": [
      "<rootDir>/src/setup.js"
    ],
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ],
    "testRegex": "^.+\\.spec\\.(js|jsx)",
    "transformIgnorePatterns": [
      "/node_modules/(?!test-component).+\\.js$"
    ]
  }
}
